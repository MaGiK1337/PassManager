<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" maxHeight="400.0" maxWidth="400.0" prefHeight="400.0" prefWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.controllers.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Text fx:id="txEnterRegMasterKey" fill="#dd0000" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter MasterKey" textAlignment="CENTER" wrappingWidth="150.0">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <TextField fx:id="inputMasterKey" alignment="CENTER">
         <VBox.margin>
            <Insets left="45.0" right="45.0" />
         </VBox.margin>
      </TextField>
      <Button fx:id="btEnterRegMasterKey" mnemonicParsing="false" text="Create" />
   </children>
</VBox>
